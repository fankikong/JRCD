<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="@dimen/listImagen"
    android:paddingRight="@dimen/normalMargin"
    tools:context="com.example.jose.layouts.Principal"
    android:id="@+id/relative">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/listImagen"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingBottom="@dimen/marg8"
        android:paddingLeft="@dimen/normalMargin"
        android:paddingRight="@dimen/normalMargin"
        android:paddingTop="@dimen/marg8"
        app:srcCompat="@drawable/dollar" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/listImagen"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:gravity="center_vertical"
        android:text="Hello World!"
        android:textSize="16sp"/>



</LinearLayout>
